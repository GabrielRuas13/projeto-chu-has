CREATE TABLE TBL_ITEM(
     ID_ITEM SERIAL PRIMARY KEY,
     TX_TITULO VARCHAR(30) NOT NULL,
     TX_DESCRICAO VARCHAR(100) NOT NULL,
     NR_FATOR_ADULTO INT NOT NULL,
     NR_FATOR_CRIANCA INT NOT NULL,
     TP_ITEM VARCHAR(14) NOT NULL,
     DT_INATIVACAO TIMESTAMP
);


